<Window x:Class="ZdravoCorp.Nurses.ChooseAppointment"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ZdravoCorp.Nurses"
        mc:Ignorable="d"
        Title="Choose an appointment" Height="450" Width="800">
    <Grid>
        <Label Content="Choose appointment:" HorizontalAlignment="Left" Margin="206,161,0,0" VerticalAlignment="Top"/>
        <ComboBox x:Name="ComboBoxAppontments" HorizontalAlignment="Left" Margin="356,163,0,0" VerticalAlignment="Top" Width="120" SelectedIndex="0"/>
        <Button x:Name="ButtonChoose" Content="Choose" HorizontalAlignment="Left" Margin="206,232,0,0" VerticalAlignment="Top" Width="82" Click="ButtonChoose_Click"/>
        <Button x:Name="ButtonCancel" Content="Cancel" HorizontalAlignment="Left" Margin="394,232,0,0" VerticalAlignment="Top" Width="82" Click="ButtonCancel_Click"/>

    </Grid>
</Window>
